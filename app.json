{
  "expo": {
    "name": "Algogo",
    "slug": "algogo-app",
    "owner": "nenu18",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-final.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F7FB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raidea.algogo",
      "buildNumber": "20",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#F6F7FB"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.raidea.algogo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-iap",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b49a5af4-877d-4a86-bae1-a075254c66e7"
      }
    }
  }
}
